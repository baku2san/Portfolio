{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch and Debug Blazor WebAssembly",
      "type": "blazorwasm",
      "request": "launch",
      "cwd": "${workspaceFolder}/src/Portfolio",
      "browser": "edge",
      "trace": true
    },
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Portfolio/bin/Debug/net9.0/Portfolio.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Portfolio",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
